{
    "name": "megaverse/laravel-vue-i18n-generator",
    "license": "MIT",
    "description": "Generates a vue-i18n compatible include file from your Laravel translations.",
    "keywords": ["laravel","vue-i18n"],
    "homepage": "http://github.com/megaversebe/laravel-vue-i18n-generator",
    "authors": [
        {
            "name": "Daniel Mendoza",
            "email": "daniel@danielmlozano.com"
        },
        {
            "name": "Daniel Janik",
            "email": "dawid.janik@megaverse.be"
        }
    ],
    "require": {
        "php": "^7.3.0|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "illuminate/console": ">=8.0",
        "illuminate/support": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "Megaverse\\VueInternationalizationGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Megaverse\\VueInternationalizationGenerator\\GeneratorProvider"
            ]
        }
    }
}
